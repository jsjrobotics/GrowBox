<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FanView">
        <attr name="hideRunningPercentage" format="boolean" />
        <attr name="percentageHeight" format="dimension" />
        <attr name="percentageWidth" format="dimension" />
        <attr name="percentageColor" format="integer" />
        <attr name="fanColor" format="integer" />
    </declare-styleable>

    <declare-styleable name="HorizontalAxis">
        <attr name="spaceBetweenDashes" format="dimension" />
        <attr name="numberOfDashes" format="integer"/>
        <attr name="isVertical" format="boolean"/>

    </declare-styleable>

    <declare-styleable name="GraphView">
        <attr name="xPartitions" format="integer" />
        <attr name="yPartitions" format="integer" />
        <attr name="axisThickness" format="dimension"/>
        <attr name="dashThickness" format="dimension" />

    </declare-styleable>

</resources>